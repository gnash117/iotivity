From 5a8ce429a6c1f8f6ba4313a9b70d8ddfa2471663 Mon Sep 17 00:00:00 2001
From: Oleksii Beketov <ol.beketov@samsung.com>
Date: Wed, 25 Sep 2019 16:29:59 +0300
Subject: [PATCH] [IOT-3301] Update unit tests

Change CSR encoding from DER to PEM format

Change-Id: I4f4f373c5a272df3b37f118531528cd5dc29da8b
Signed-off-by: Oleksii Beketov <ol.beketov@samsung.com>
---
 .../csdk/security/provisioning/unittest/secureresourceprovider.cpp    | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/resource/csdk/security/provisioning/unittest/secureresourceprovider.cpp b/resource/csdk/security/provisioning/unittest/secureresourceprovider.cpp
index 67ee08353..123f17da7 100644
--- a/resource/csdk/security/provisioning/unittest/secureresourceprovider.cpp
+++ b/resource/csdk/security/provisioning/unittest/secureresourceprovider.cpp
@@ -995,7 +995,7 @@ TEST_F(SRP, registerResultForGetCSRResourceCBFull)
     getCsrData->resArr = (OCPMGetCsrResult_t *)OICCalloc(1, sizeof(OCPMGetCsrResult_t));
     getCsrData->resArr->csr = certData();
     getCsrData->resArr->csrLen = certDataLen();
-    getCsrData->resArr->encoding = OIC_ENCODING_DER;
+    getCsrData->resArr->encoding = OIC_ENCODING_PEM;
 
     uint8_t *payload = NULL;
     size_t payloadSize = 0;
@@ -1014,7 +1014,7 @@ TEST_F(SRP, registerResultForGetCSRResourceCBNull)
     getCsrData->resArr = (OCPMGetCsrResult_t *)OICCalloc(1, sizeof(OCPMGetCsrResult_t));
     getCsrData->resArr->csr = certData();
     getCsrData->resArr->csrLen = certDataLen();
-    getCsrData->resArr->encoding = OIC_ENCODING_DER;
+    getCsrData->resArr->encoding = OIC_ENCODING_PEM;
 
     uint8_t *payload = NULL;
     OCStackResult stackresult = OC_STACK_OK;
-- 
