+MPMResult deleteVirtualServer(pid_t pid)
+{
+#ifdef SECURED
+    deleteOwnedStateOnSharedMemory(pid);
+#endif
+    kill(pid, SIGUSR1);
+    sleep(2);
+
+    return MPM_RESULT_OK;
+}
