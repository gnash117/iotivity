+#define HEXADECIMAL_BUFFER_LENGTH 8
+#define PUREPAYLOAD_HEADER_LENGTH 8
+#define DATATYPE_START_INDEX_PUREPAYLOAD 6
+#define DATATYPE_LENGTH 2
