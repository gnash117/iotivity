+int ZigbeeDevice::getUINT8(std::string str)
+{
+    int v;
+    stringstream ss;
+    ss.str(str);
+    ss >> std::hex >> v;
+
+    return v;
+}
+
+int ZigbeeDevice::getUINT16(std::string str)
+{
+    if(str.length() > 0)
+    {
+        int hex1, hex2;
+        stringstream ss1, ss2;
+        ss1.str(str.substr(0, 2));
+        ss1 >> std::hex >> hex1;
+        ss2.str(str.substr(2, 2));
+        ss2 >> std::hex >> hex2;
+        return hex2 * 256 + hex1;
+    }
+    else
+    {
+        return 0;
+    }
+}
